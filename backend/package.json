{
  "name": "virtual-number-otp-backend",
  "version": "1.0.0",
  "description": "Backend service for virtual number OTP application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "kill-port": "powershell -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 5000).OwningProcess | Stop-Process -Force\"",
    "kill-5000": "npm run kill-port",
    "kill-all": "powershell -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 5000,3000).OwningProcess | Stop-Process -Force\"",
    "test-swagger": "node test-swagger.js",
    "check-5sim": "node check-5sim-availability.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "virtual-number",
    "otp",
    "sms",
    "twilio",
    "5sim",
    "sms-activate"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
